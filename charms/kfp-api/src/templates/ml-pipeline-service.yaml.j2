apiVersion: v1
kind: Service
metadata:
  name: ml-pipeline
  namespace: '{{ namespace }}'
spec:
  ports:
  - name: grpc 
    port: {{ grpc_port }}
    protocol: TCP
    targetPort: 8887
  - name: http
    port: {{ http_port }}
    protocol: TCP
    targetPort: 8888
  selector:
    app.kubernetes.io/name: {{ service }}
